<p id="notice"><%= notice %></p>

<h1>Order Books</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_books.each do |order_book| %>
      <tr>
        <td><%= order_book.user_id %></td>
        <td><%= order_book.book_id %></td>
        <td><%= link_to 'Show', order_book %></td>
        <td><%= link_to 'Edit', edit_order_book_path(order_book) %></td>
        <td><%= link_to 'Destroy', order_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Book', new_order_book_path %>
